{
	"name": "atomjoy/smsapisms",
	"description": "SMSAPI SMS Laravel notification channel.",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"classmap": [
			"src/"
		],
		"psr-4": {
			"Atomjoy\\Sms\\": "src/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Migrations\\": "database/migrations/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Atomjoy\\Sms\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Atomjoy",
			"email": "atomjoy.official@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": "^8.1",
		"smsapi/php-client": "^3.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Atomjoy\\Sms\\SmsServiceProvider"
			]
		}
	},
	"scripts": {
		"post-smsapisms-cmd": []
	}
}
